# Database credentials
POSTGRES_USER=sec
POSTGRES_PASSWORD=secpass
POSTGRES_DB=secdb
MONGO_INITDB_DATABASE=secmongo

# Connection URLs consumed by services
# Example Postgres URL for FastAPI (psycopg/SQLAlchemy):
POSTGRES_URL=postgresql+psycopg://sec:secpass@postgres:5432/secdb
# Example Mongo URL for Motor/Mongoose:
MONGODB_URL=mongodb://mongodb:27017/secmongo
# Redis and RabbitMQ URLs
REDIS_URL=redis://redis:6379/0
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/

# Service-specific
FASTAPI_LOG_LEVEL=info
NESTJS_PORT=3000

# n8n
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_BASE_URL=/n8n
N8N_DIAGNOSTICS_ENABLED=false
N8N_EXECUTIONS_MODE=regular
