{
  "name": "sec",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:debug": "tsc -b --verbose && vite build --debug",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "update": "npm run update:deps && npm run update:build",
    "update:deps": "npx npm-check-updates -u && npm install",
    "update:build": "npm run build",
    "update:all": "npm run update:deps && npm run build && npm run lint",
    "check-updates": "npx npm-check-updates",
    "security-audit": "npm audit && echo 'Checking for vulnerabilities...'",
    "maintenance": "npm run check-updates && npm run security-audit && npm run update:all"
  },
  "dependencies": {
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.4",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "npm-check-updates": "^19.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "npm:rolldown-vite@7.1.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.16"
  }
}
